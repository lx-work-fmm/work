import numpy as np
N = 12

y1 = np.exp(np.linspace(0,N-1,N)/4)
noise = (5 * np.random.rand(N)-1) - 2.5
y2 = y1 + noise

y1 = y1.reshape((N,1))
y2 = y2.reshape((N,1))

import matlab
y1_matlab = matlab.double(y1.tolist())
y2_matlab = matlab.double(y2.tolist())

arr = np.array(range(10))

print(np.concatenate((arr[2:], arr[:2])))
print(np.roll(arr, -2))